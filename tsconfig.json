{
  "files": [],
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@payload-config": [
        "./src/payload.config.ts"
      ]
    },
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": [
      "./@types"
    ]
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "./dist/types/**/*.ts",
    "./next-env.d.ts",
    "src",
    "src/components",
    "src/custom.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "./node_modules"
  ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    }
    //   {
    //     "path": "./tsconfig.node.json"
    //   }
  ]
}